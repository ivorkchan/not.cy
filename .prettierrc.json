{
  "quoteProps": "preserve",
  "htmlWhitespaceSensitivity": "strict",
  "plugins": ["@ianvs/prettier-plugin-sort-imports"],
  "importOrder": [
    "^(react/(.*)$)|^(react$)",
    "^@react/(.*)$",
    "^(next/(.*)$)|^(next$)",
    "^@next/(.*)$",
    "<BUILTIN_MODULES>",
    "<THIRD_PARTY_MODULES>",
    "",
    "^@/app/(.*)$",
    "^@/lib/(.*)$",
    "^@/utils/(.*)$",
    "^@/configs/(.*)$",
    "^@/contents/(.*)$",
    "",
    "^@/components/(.*)$",
    "^@/components/filter/(.*)$",
    "^@/components/animate/(.*)$",
    "^@/components/embed/(.*)$",
    "^@/components/mdx/(.*)$",
    "^@/components/neodb/(.*)$",
    "",
    "<TYPES>",
    "<TYPES>^[.]"
  ],
  "importOrderParserPlugins": ["typescript", "jsx", "decorators-legacy"]
}
