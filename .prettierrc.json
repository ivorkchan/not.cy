{
  "quoteProps": "preserve",
  "htmlWhitespaceSensitivity": "strict",
  "plugins": ["@ianvs/prettier-plugin-sort-imports"],
  "importOrder": [
    "^(react/(.*)$)|^(react$)",
    "^(next/(.*)$)|^(next$)",
    "",
    "<BUILTIN_MODULES>",
    "<THIRD_PARTY_MODULES>",
    "",
    "<TYPES>",
    "<TYPES>^[.]",
    "",
    "^@/lib/(.*)$",
    "^@/components/mdx/(.*)$",
    "^@/components/(.*)$",
    "^@/contents/(.*)$",
    "^@/app/(.*)$"
  ],
  "importOrderParserPlugins": ["typescript", "jsx", "decorators-legacy"]
}
