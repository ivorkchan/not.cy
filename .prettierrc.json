{
  "endOfLine": "lf",
  "semi": false,
  "singleQuote": false,
  "tabWidth": 2,
  "trailingComma": "es5",
  "quoteProps": "preserve",
  "plugins": ["@ianvs/prettier-plugin-sort-imports"],
  "importOrder": [
    "^(react/(.*)$)|^(react$)",
    "^(next/(.*)$)|^(next$)",
    "",
    "<BUILTIN_MODULES>",
    "<THIRD_PARTY_MODULES>",
    "",
    "<TYPES>",
    "<TYPES>^[.]",
    "",
    "^@/lib/(.*)$",
    "^@/components/mdx/(.*)$",
    "^@/components/(.*)$",
    "^@/contents/(.*)$",
    "^@/app/(.*)$"
  ],
  "importOrderParserPlugins": ["typescript", "jsx", "decorators-legacy"]
}
